{
    "name": "aymane/converter-cli",
    "description": "a prototype of a calculator that converts from decimal to binary and hexadecimal",
    "autoload": {
        "psr-4": {
            "App\\":"src/"
        }
    },
    "scripts": {
        "convert":"php bin/convert.php",
        "shiftleft":"php bin/convert.php 42 --shl",
        "shiftright":"php bin/convert.php 42 --shr"
    },
    "require": {
        "php":">=8.2"
    }
}